<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.TbOaMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.entity.TbOa">
        <id column="id" property="id" />
        <result column="title" property="title" />
        <result column="content" property="content" />
        <result column="timestamp" property="timestamp" />
        <result column="login_id" property="loginId" />
        <result column="last_name" property="lastName" />
        <result column="subcompany_name" property="subcompanyName" />
        <result column="department_name" property="departmentName" />
        <result column="keywords" property="keywords" />
        <result column="favored_count" property="favoredCount" />
        <result column="read_count" property="readCount" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, title, content, timestamp, login_id, last_name, subcompany_name, department_name, keywords, collect_number, read_count, create_time, update_time
    </sql>

</mapper>
